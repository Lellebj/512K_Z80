                        	   435: 		; 							72 cycles if a character is available
                        	   436: 		; 							INST
                        	   437: 		; 							27 cycles
                        	   438: 		; 							OUTCH
                        	   439: 		; 							150 cycles if output buffer is not full
                        	   440: 		; 								and output interrupt is expected
                        	   441: 		; 							OUTST
