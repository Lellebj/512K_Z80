                        	   480: ; 		RRA                         ;SET CARRY FROM DATA READY FLAG
                        	   481: ; 									: IF CARRY = 1, CHARACTER IS AVAILABLE
                        	   482: ; 		RET
                        	   483: ; 		; WRITE CHARACTER
                        	   484: ; OUTCH:
                        	   485: ; 		PUSH      AF                :SAVE CHARACTER TO WRITE
                        	   486: 
